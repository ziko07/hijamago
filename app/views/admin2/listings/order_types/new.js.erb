<% if @template.present? %>
  $('#category-body').html('<%= j render 'admin2/listings/order_types/category_body', locals: @locals %>');
  $('.order-type-footer').prop('disabled', false);
  window.ST.initializeListingShapeForm('#listing_shape_form');
  $('#template_order_type option[value=""]').hide();
<% else %>
  $('#category-body').html('');
  $('.order-type-footer').prop('disabled', true);
<% end %>
