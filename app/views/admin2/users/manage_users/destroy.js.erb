<% if @error.present? %>
  showError('<%= j @error %>');
<% else %>
  $('.membership-row-<%= @service.membership.id %>').remove();
  $('#userDeleteModal').modal('hide');
  var current = $('#status_').val();
  $('#user_statuses_filter').html('<%= j render 'admin2/users/manage_users/statuses', presenter: @presenter, current: nil %>');
  $('#status_').val(current);
<% end %>
