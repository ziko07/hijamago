- content_for :popup_layout do
  #testDNSsettings.modal{"aria-hidden" => "true", "aria-labelledby" => "testDNSsettingsTitle", "data-backdrop" => "static", :role => "dialog", :tabindex => "-1"}
    .modal-dialog.modal-md{role: :document}
      = form_for @presenter.community, url: url, html: { class: 'modal-content' } do |form|
        = form.hidden_field :domain
        .modal-header
          %h2#testDNSsettingsTitle.modal-title
          %button.close{"aria-label" => "Close", "data-dismiss" => "modal", :type => "button"}
            %span{"aria-hidden" => "true"} Ã—
        .modal-body
          %p.ingress=t('admin2.domain.test_dns_popup.info')
          %br
          %p.ingress=t('admin2.domain.test_dns_popup.info_2')
        .modal-footer
          = link_to t('admin2.cancel'), '#', class: %i[cancel-modal], 'data-dismiss': :modal
          = button_tag btn_text, class: %i[btn btn-primary]
