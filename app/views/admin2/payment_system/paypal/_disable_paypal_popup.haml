#disablePaypalModal.modal{'aria-hidden' => 'true', 'aria-labelledby' => "disablePaypalLabel", 'data-backdrop' => 'static', role: :dialog, tabindex: '-1'}
  .modal-dialog.modal-md{role: :document}
    = form_tag disable_admin2_payment_system_paypal_path(payment_gateway: :paypal), method: :patch, class: %i[modal-content], style: 'margin-left:0;' do
      .modal-header
        %h2.modal-title{id: 'disablePaypalLabel'}
          = t('admin2.paypal.disable_paypal')
        %button.close{'aria-label' => 'Close', 'data-dismiss' => 'modal', type: :button}
          %span{'aria-hidden' => 'true'} Ã—
      .modal-body
        = t('admin2.stripe.confirm_disable', gateway: 'PayPal')
        %br
      .modal-footer
        = link_to t('admin2.cancel'), '#', class: %i[cancel-modal], 'data-dismiss': :modal
        = button_tag t('admin2.paypal.disable_paypal'), class: %i[btn btn-negative]
